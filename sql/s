     document\n                .getElementById(\"dolores-table\")\n                .appendChild(tbody);\n        }, timeout)\n    }\n\n    //\u6570\u636e\u52a0\u8f7d\u5c42\n    function loading() {\n        // \u6a21\u62df\u5f02\u6b65\u52a0\u8f7d\u6570\u636e\n        setTimeout(function () {\n            $('#data-loading').hide(); // \u9690\u85cf loading \u6548\u679c\n        }, timeout);\n    }\n\n    //\u5c55\u793a\u52a0\u8f7d\u5c42\n    function showLoading() {\n        $('#data-loading').show();\n    }\n\n    //\u9690\u85cf\u52a0\u8f7d\u5c42\n    function hideLoading() {\n        // \u6a21\u62df\u5f02\u6b65\u52a0\u8f7d\u6570\u636e\n        setTimeout(function () {\n            $('#data-loading').hide(); // \u9690\u85cf loading \u6548\u679c\n        }, timeout);\n    }\n\n    function adjustCellHeight() {\n        $('#dolores-table td').each(function () {\n            var $cell = $(this);\n            var $content = $cell.children('.cell-content');\n            var cellHeight = $content.outerHeight();\n            $cell.css('height', cellHeight + 'px');\n        });\n    }\n\n    // \u5728\u5355\u5143\u683c\u4e2d\u6dfb\u52a0\u5185\u5bb9\u65f6\uff0c\u8c03\u7528\u8be5\u51fd\u6570\n    function addCellContent($cell, content) {\n        var $content = $('<div class=\"cell-content\">' + content + '</div>');\n        $cell.empty().append($content);\n        adjustCellHeight();\n    }"]}, "end_turn": null, "weight": 1.0, "metadata": {"timestamp_": "absolute", "message_type": null}, "recipient": "all"}, "parent": "7c855e55-aec4-4271-95ac-677e8cc4f35c", "children": ["0e387874-715f-4dbc-be8e-940e6e0f18df"]}, "0e387874-715f-4dbc-be8e-940e6e0f18df": {"id": "0e387874-715f-4dbc-be8e-940e6e0f18df", "message": {"id": "0e387874-715f-4dbc-be8e-940e6e0f18df", "author": {"role": "assistant", "name": null, "metadata": {}}, "create_time": 1683517403.392938, "update_time": null, "content": {"content_type": "text", "parts": ["\u5728\u4f60\u7684\u4ee3\u7801\u4e2d\uff0c\u4f60\u4f7f\u7528\