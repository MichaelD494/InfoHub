//\u6c61\u67d3\u7269\u503c\n        String pollutantValue = param.get(\"pollutantValue\").toString();\n        //\u6c61\u67d3\u7269\u5b57\u6bb5\u540d\n        String pollutantPropertyName = param.get(\"pollutantPropertyName\").toString();\n        //\u83b7\u53d6\u901a\u7528\u5de5\u5382map\n        Map<String, Object> commonFactoryMap = getCommonFactoryResource();\n        //\u4ece\u901a\u7528\u5de5\u5382map\u83b7\u53d6\u6821\u9a8cExcel\u5de5\u5382\n        VerifyExcel verifyExcelFactory = (VerifyExcel) commonFactoryMap.get(\"verifyExcelFactory\");\n        //\u521b\u5efa\u6c61\u67d3\u7269\u6570\u636e\u4f20\u8f93\u5bf9\u8c61\n        PollutantDto dto = null;\n        //\u521b\u5efa\u65b9\u6cd5\u8fd4\u56de\u6570\u636emap\n        Map<String, Object> data = new HashMap<>();\n        //\u5224\u65ad\u6c61\u67d3\u7269\u503c\u662f\u5426\u4e3a\u7a7a\n        if (StringUtils.isNotBlank(pollutantValue)) {\n            //\u904d\u5386\u6c61\u67d3\u7269\u96c6\u5408\n            for (CalWaterLevelPollutant item : pollutantList) {\n                //\u67e5\u627e\u76ee\u6807\u6c61\u67d3\u7269\n                if (item.getPollutantCode().equals(pollutantCode)) {\n                    //\u5224\u65ad\u6c61\u67d3\u7269\u503c\u662f\u5426\u5e26L && \u5ffd\u7565\u96c6\u5408\u662f\u5426\u5305\u542b\u6c61\u67d3\u7269\u5b57\u6bb5\u540d\n                    if (pollutantValue != null && !pollutantValue.contains(\"L\") && !ignoreList.contains(pollutantPropertyName)) {\n                        //\u8f6c\u6362\u6210double\u7c7b\u578b\n                        double tmp = Double.parseDouble(pollutantValue);\n                        //\u5224\u65ad\u503c\u662f\u5426\u4e3a-1\u6216\u8005-3\n                        if (tmp == -1 || tmp == -3) {\n                            //\u5982\u679c\u662f\u91cd\u7f6e\u6c61\u67d3\u7269\u503c\n                            pollutantValue = null;\n                        }\n                    }\n                    //\u5224\u65ad\u6c61\u67d3\u7269\u503c\u662f\u5426\u53